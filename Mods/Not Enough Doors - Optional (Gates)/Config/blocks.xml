	<property name="MultiBlockDim" value="5,5,1"/>
	<property name="ImposterExchange" value="imposterDontBlockCTRPlate" param1="12"/>
	<property name="LODCullScale" value=".1"/>
	<property name="MeshDamage" value="
		Door/DMG0, 10000
		"/>

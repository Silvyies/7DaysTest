<PaperSama0104>

<!-- 
SortOrder2 odd values are reserved for non-barricaded aka "clean" versions.

If you want to edit the health of the gates, see the following for consistency:
// WOOD //
* NEDwoodGate_chainlinkGateDouble				;	MaxDamage, MeshDamage
  > NEDwoodGate_chainlinkGateDoubleWide			;	MeshDamage
  > NEDwoodGate_doorWoodLargeGate				;	MeshDamage
// IRON, STEEL, STEEL_POWERED //
! iron, steel, and steel_powered follows the above blocks,
! just use NEDiron, NEDsteel, and NEDsteel_Powered instead
 -->

<append xpath="/blocks">
<!-- *** xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->
<!-- **** NED_WOOD -->
<!-- *** xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->

	<!-- *** Gates -->
<block name="NEDwoodGate_chainlinkGateDouble">
	<property name="Extends" value="NEDwoodDoor_oldWoodDoor"/>
	<property name="OpenSound" value="gate_chainlink_open"/>
	<property name="CloseSound" value="gate_chainlink_close"/>
	<property name="LockedSound" value="gate_chainlink_locked"/>
	<property name="CustomIcon" value="chainlinkGateDouble"/>
	<property name="Material" value="Mwood_regularGarageDoor3x2x2"/>
	<property name="Model" value="Entities/Buildings/chainlinkGateDoublePrefab"/>
	<property name="MultiBlockDim" value="5,3,1"/>
	<property name="Collide" value="movement,melee,rocket"/>
	<property name="MaxDamage" value="10000"/>
	<property name="MeshDamage" value="
		Door/DMG0, 10000,
		Door/DMG1, 2500,
		Door/DMG2, 1250,
		-, 1
		"/>
	<property class="RepairItems">
		<property name="resourceWood" value="30"/>
	</property>
	<property class="UpgradeBlock">
		<property name="ToBlock" value="NEDironGate_chainlinkGateDouble"/>
		<property name="Item" value="NEDironGateBlockVariantHelper"/>
		<property name="ItemCount" value="1"/>
		<property name="UpgradeHitCount" value="5"/>
	</property>
	<drop event="Harvest" name="resourceWood" count="15,30" tag="allHarvest,lumberjackHarvest"/>
	<property name="SortOrder1" value="B388"/>
	<property name="SortOrder2" value="0002"/>
</block>

<block name="NEDwoodGate_chainlinkGateDoubleWide">
	<property name="Extends" value="NEDwoodGate_chainlinkGateDouble" param1="Stage2Health,RepairItemsMeshDamage"/>
	<property name="OpenSound" value="gate_chainlink_trader_open"/>
	<property name="CloseSound" value="gate_chainlink_trader_close"/>
	<property name="CustomIcon" value="chainlinkGateDoubleWide"/>
	<property name="Model" value="Entities/Buildings/chainlinkGateWidePrefab"/>
	<property name="ModelOffset" value="0,0,-0.5"/>
	<property name="MeshDamage" value="
		Door/DMG0, 10000,
		Door/DMG1, 2500,
		Door/DMG2, 1250
		"/>
	<property class="UpgradeBlock">
		<property name="ToBlock" value="NEDironGate_chainlinkGateDoubleWide"/>
		<property name="Item" value="NEDironGateBlockVariantHelper"/>
		<property name="ItemCount" value="1"/>
		<property name="UpgradeHitCount" value="5"/>
	</property>
	<property name="SortOrder2" value="0004"/>
</block>

<block name="NEDwoodGate_doorWoodLargeGate">
	<property name="Extends" value="NEDwoodGate_chainlinkGateDoubleWide" param1="Collide"/>
